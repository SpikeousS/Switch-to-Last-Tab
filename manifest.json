{
  "manifest_version": 3,
  "name": "Switch to Last Tab",
  "version": "1.0.0",
  "description": "Switches to the last visited tab",
  "permissions": [
    "tabs",
    "tabGroups",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "open-last-tab": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Command+B"
      },
      "description": "Switch to the last visited tab"
    }
  },
  "action": {
    "default_icon": {
      "16": "images/SwitchTabLogo16.png",
      "48": "images/SwitchTabLogo48.png",
      "128": "images/SwitchTabLogo128.png"
    },
    "default_title": "Switch to Last Tab",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/SwitchTabLogo16.png",
    "48": "images/SwitchTabLogo48.png",
    "128": "images/SwitchTabLogo128.png"
  }
}